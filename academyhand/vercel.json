{
  "buildCommand": "cd academyhand && npm install && npm run build",
  "outputDirectory": "academyhand/.next",
  "devCommand": "cd academyhand && npm run dev",
  "installCommand": "cd academyhand && npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/academyhand/$1"
    }
  ]
}